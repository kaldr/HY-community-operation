//out: ../../../lib/test/css/app.css, sourcemap: false, compress: true, libs: axis
.toolbar
  position fixed
  bottom 0
  padding 30px
  background-color rgb(238, 238, 238,0.5)
  border-top 1px solid #CCC
  width:100%
.option
  margin-bottom:10px
.btn-long
  width:100%
div.scrollDiv
  height:180px
.tip
  box-shadow:0px 0px 5px #777
  text-align: center
  width:100%
  position fixed
  top 0
  padding 15px

.titleContainer
  text-align: center
  p
    color #777
    font-size 18px
.bannerContainer
  margin-bottom: 20px
  img.nosee
    position:absolute
    left:-10000px
    top:-10000px
.bannerContainer
  text-align: center
  margin 0 auto

body
  background url('../images/1_047.gif')
.questionImgDiv
  padding: 50px
#questions
  margin-top:30px
  background-color: #fff
  border-top 1px solid #ddd
  .questionTitle
    margin-bottom: 10px
  .questionsContainer
    h2,h3,h4
      text-align: center;
      font-size 22px
      width:100%
      padding 20px
      padding-bottom 0
      margin:0
      background:url('../images/1_011.gif') #58ADAA
      color:white
    h2 i
      font-size 18px
      top 2px
      margin-right 5px
    h3
      text-align: left
      padding-bottom 20px
      font-size 18px
      margin-bottom 20px
#answers
  margin-top 30px
  background: transparent
  div.container
    padding:30px 50px
    .row
      margin-bottom 20px
      h2
        text-align: center;
        font-size 22px
        width:100%
        padding 20px
        margin:0
        background:url('../images/1_011.gif') #58ADAA
        color:white
      img
        padding:30px
        background-color: #fff
        border 1px solid #ccc
        width:100%
      h2.finished
        background-image: none
        background-color: #58ADAA
        border 1px solid #5F9FAF
