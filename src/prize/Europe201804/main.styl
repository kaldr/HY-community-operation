//out:../../../lib/prize/oldMonitors/main.css
body {
  margin: 0;
  font: normal 75% Arial, Helvetica, sans-serif;
}
canvas {
  display: block;
  vertical-align: bottom;
}
/* ---- particles.js container ---- */
#particles-js {
  position: absolute;
  width: 100%;
  height: 100%;
  background: radial-gradient(#b61924, #b61924, #910a14);
  background-size: cover;
  background-position: 50% 50%;
}
.mr15 {
  margin-right: 15px;
}
.container {
  z-index: 100;
  position: absolute;
  top: 50%;
  left: 50%;
  display: block;
  width: 900px;
  height: 600px;
  margin: 0;
  margin-top: -300px;
  margin-left: -450px;
  text-align: center;
  background-color: rgba(255,255,255,0.8);
  box-shadow: 0 0px 15px #777;
  padding: 20px;
  border-radius: 10px;
  background-image: url('./bg.png')
  background-repeat: no-repeat;
  background-position: center bottom
}
.container #title img {
  position: relative;
  top: -80px;
}
.container #buttons {
  width: 500px;
  margin-left: -250px;
  margin: 0 auto;
  position: relative;
  display: block;
  margin-bottom: 30px;
}
.container
  .panel
    h3
      font-size 18px
    .panel-body
      font-size 16px
      display block
      span.label
        display: block
        float: left
        margin-right 12px
        font-size 26px
        margin-bottom 12px
  #luckers
    display: none
  #prizeLeft
    position relative
    top -40px
    font-size: 30px
    #left
      margin 0 20px
      font-size 50px
      color #b61924
  #buttons
    .stop
      display none
  #dashboard
    position relative
    top -40px
    #numbers
    #result
      font-size 130px
    #result
      display none
